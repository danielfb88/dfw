<?xml version="1.0" encoding="UTF-8"?>
<!--
Configuração da estrutura física das classes Command
-->
<root>
    <command name="default">
        <classname>DefaultCommand</classname>
        <filepath>Controller/DefaultCommand.class.php</filepath>
    </command>
    <command name="login">
        <classname>LoginCommand</classname>
        <filepath>Controller/LoginCommand.class.php</filepath>
    </command>
    <command name="teste" module="teste" submodule="teste">
        <classname>TesteCommand</classname>
        <filepath>Controller/TesteCommand.class.php</filepath>
    </command>
</root>
